{
  "name": "react-ts-standalone",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --open --progress",
    "test": "jest --passWithNoTests --no-cache --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --no-cache --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primereact": "^6.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.3",
    "eslint": "7",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "11",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.4.0",
    "file-loader": "^6.2.0",
    "git-commit-msg-linter": "^3.0.3",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.18",
